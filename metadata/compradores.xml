<?xml version="1.0" encoding="UTF-8"?>
<LAYER alias="compradores">
       <TABLENAME>compradores</TABLENAME>
       <PRIMARYKEY>
	<PKELEMENT>gid</PKELEMENT>
       </PRIMARYKEY>

       <RECORDSET>
		<FIELD>
			<FIELDNAME>cod_comprador</FIELDNAME>
			<VALIDATIONRULE>MANDATORY</VALIDATIONRULE>
	    </FIELD>
	    <FIELD>
			<FIELDNAME>nombre</FIELDNAME>
			<VALIDATIONRULE>MANDATORY</VALIDATIONRULE>
	    </FIELD>
       	    <FIELD>
		<FIELDNAME>x</FIELDNAME>
		<VALIDATIONRULE>DOUBLE_POSITIVE</VALIDATIONRULE>
	    </FIELD>
       	    <FIELD>
		<FIELDNAME>y</FIELDNAME>
		<VALIDATIONRULE>DOUBLE_POSITIVE</VALIDATIONRULE>
	    </FIELD>
       	    <FIELD>
		<FIELDNAME>z</FIELDNAME>
		<VALIDATIONRULE>DOUBLE_POSITIVE</VALIDATIONRULE>
	    </FIELD>
<FIELD>
		<FIELDNAME>compra_total</FIELDNAME>
		<VALIDATIONRULE>DOUBLE_POSITIVE</VALIDATIONRULE>
	    </FIELD>
       </RECORDSET>

</LAYER>
